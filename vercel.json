{
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "rewrites": [
    {
      "source": "/api/(.*)", 
      "destination": "/api/index"
    },
    {
      "source": "/(.*)", 
      "destination": "/src/$1"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  },
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  }
}


